{
  "name": "projeto-devs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:open": "ng serve --open",
    "install:dependencies": "npm install",
    "prettier": "prettier --write src/**/*.{ts,js,css,html}",
